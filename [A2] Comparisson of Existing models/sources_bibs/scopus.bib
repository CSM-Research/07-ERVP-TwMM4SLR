	title = {Paving the Green Path: A Maturity Model for Digital Decarbonization},
	year = {2024},
	journal = {Procedia CIRP},
	volume = {130},
	pages = {1020 - 1027},
	doi = {10.1016/j.procir.2024.10.201},
