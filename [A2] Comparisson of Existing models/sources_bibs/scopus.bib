@ARTICLE{Li2024,
	author = {Li, Tao and Rui, Yi and Zhao, Sicheng and Zhang, Yudong and Zhu, Hehua and Li, Xiaojun},
	title = {A quantitative digital twin maturity model for underground infrastructure based on D-ANP},
	year = {2024},
	journal = {Tunnelling and Underground Space Technology},
	volume = {146},
	pages = {},
	doi = {10.1016/j.tust.2024.105612},
