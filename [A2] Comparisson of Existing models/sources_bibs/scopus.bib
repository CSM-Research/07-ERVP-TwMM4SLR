	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus}
}

@ARTICLE{Ojubanire2025,
	author = {Ojubanire, Olugbenga Ayo and Berbain, Sabrina and Sebti, Hicham},
	title = {Manufacturing enterprises in Industry 4.0 era: a maturity model for emerging economies},
	year = {2025},
	journal = {Production and Manufacturing Research},
	volume = {13},
