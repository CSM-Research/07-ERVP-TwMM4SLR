@ARTICLE{Ünlu2024,
	author = {Ünlu, Huseyin and Demirors, Onur and Garousi, Vahid},
	title = {Readiness and maturity models for Industry 4.0: A systematic literature review},
	year = {2024},
	journal = {Journal of Software: Evolution and Process},
