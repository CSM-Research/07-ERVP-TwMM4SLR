@ARTICLE{Mazimwe2021,
	author = {Mazimwe, Allan and Hammouda, Imed and Gidudu, Anthony},
	title = {Implementation of FAIR principles for ontologies in the disaster domain: A systematic literature review},
	year = {2021},
