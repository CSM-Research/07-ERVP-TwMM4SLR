  ISSN={},
  month={Dec},}@INPROCEEDINGS{7170443,
  author={Peres, Angela Lima and Meira, Silvio Lemos},
  booktitle={2015 10th Iberian Conference on Information Systems and Technologies (CISTI)}, 
  title={Towards a framework that promotes integration between the UX design and SCRUM, Aligned to CMMI}, 
  year={2015},
  volume={},
  number={},
  pages={1-4},
  abstract={This paper proposes a framework to integrate user experience design into the software development cycle. The proposal defines practices, principles, techniques and artifacts of user experience in organizations that use agile methodologies (SCRUM) in association with CMMI process areas and the dimensions prescribed in the maturity model for user experience design recommended by the Human Factors Institute. The evolutionary path is supported by SCRUM practices. The preliminary framework is based on systematic reviews of the integration of design and experience, agile methodologies, SCRUM and CMMI. The framework, as well as future studies, will be validated and refined by panels of experts and case studies.},
  keywords={Usability;Organizations;Software engineering;Human factors;Bibliographies;Systematics;Maturity models;CMMI;user experience;agile methodologies;SCRUM},
  doi={10.1109/CISTI.2015.7170443},
  ISSN={2166-0727},
  month={June},}@INPROCEEDINGS{10005414,
  author={Wilke, Daria and Pfeifer, Stefan Achilles and Heitmann, Rebecca and Anacker, Harald and Dumitrescu, Roman and Franke, Volker},
  booktitle={2022 IEEE International Symposium on Systems Engineering (ISSE)}, 
  title={Implementation of Systems Engineering: A maturity-based approach}, 
  year={2022},
  volume={},
  number={},
  pages={1-7},
  abstract={Due to an increasing amount of software and electronics, future technical systems will be significantly more capable than current ones, but also more complex. This increase in complexity leads to growing challenges in the context of product development. In industry, Systems Engineering (SE) is seen as a promising development approach to handle these challenges. However, despite great efforts in some cases, companies struggle with a variety of challenges when introducing SE. This paper presents an approach to facilitate the introduction of SE especially for small and medium-sized companies. For this purpose, we have developed a maturity-based implementation strategy. This strategy enables companies to determine their status and to derive strategic goals regarding their SE capability. As a first step, we conducted a Systematic Literature Review (SLR) to evaluate the current state of the art of SE based maturity models. We found four relevant approaches, which only cover partial areas of the introduction of SE. Subsequently, we present our approach, which was developed within a research project. In this model, the SE maturity level is determined in 3 assessment dimensions with the help of 20 action areas. Our approach is characterized in particular by an additional info box. It provides the evaluators with additional information and reflection questions for each action area to simplify the evaluation. Finally, we validate the maturity model with an industrial company.},
  keywords={Industries;Analytical models;Systematics;Target recognition;Instruments;Companies;Reflection;systems engineering;maturity model;implementation strategy;medium-sized companies},
  doi={10.1109/ISSE54508.2022.10005414},
  ISSN={2687-8828},
  month={Oct},}@INPROCEEDINGS{7076887,
  author={Almomani, Malek Ahmad Theeb and Basri, Shuib and Mahamad, Saipunidzam and Bajeh, Amos Orenyi},
  booktitle={2014 3rd International Conference on Advanced Computer Science Applications and Technologies}, 
  title={Software Process Improvement Initiatives in Small and Medium Firms: A Systematic Review}, 
  year={2014},
  volume={},
  number={},
  pages={162-167},
  abstract={Nowadays, software firms are playing very important roles in economies all over the world, and majority of these firms are Small and Medium Enterprises (SMEs). The SMEs recognize that it is important to improve their development processes and activities in order to produce software with high quality and to be competitive in the software industry. However, several studies have shown that the SMEs used ad-hoc manner in their development processes and they suffer from lack of the knowledge and resources to do Software Process Improvement (SPI). In this paper, a Systematic Literature Review (SLR) is used to identify and discuss the SPI initiatives in SME software firms. The results of the systematic review showed that the majority of software process improvement initiatives in SME firms are carried out in America and Europe. Only in one Asian country, Hong Kong was the SPI initiative considered and CMM Fast-Track was the model developed. The developed model did not cover all the CMM levels, only levels 1 and 2 were considered. In conclusion, many Software Process Improvement initiatives have been proposed for SME software firms but they have shortcomings such as lack of generalizability and take a lot of time to gain the benefits/advantage of the SPI model after implementation.},
  keywords={Software;Capability maturity model;Coordinate measuring machines;Systematics;Biological system modeling;Standards;Organizations;Systematic Literature Review (SLR);Software Process Improvement (SPI);Small and Medium Enterprises (SMEs)},
  doi={10.1109/ACSAT.2014.35},
