  month={Dec},}@INPROCEEDINGS{8610966,
  author={Akbar, Muhammad Azeem and Nasrullah and Shameem, Mohammad and Ahmad, Jawad and Maqbool, Amna and Abbas, Khizer},
  booktitle={2018 International Conference on Computing, Electronic and Electrical Engineering (ICE Cube)}, 
  title={Investigation of Project Administration related challenging factors of Requirements Change Management in global software development: A systematic literature review}, 
  year={2018},
  volume={},
  number={},
  pages={1-7},
  abstract={Presently, software firms are adopting global software development (GSD) paradigm increasingly, due to producing quality software's with low cost. But, the adoption of GSD is not straightforward, there are various complications allied with it mainly that related to requirements change management (RCM). The aim of this study is to identify project administration related challenges (CHs) that could negatively impact the RCM activities in GSD firms. We have employed systematic literature review (SLR) method in order to investigate the CHs. Using the SLR method, 10 CHs were identified. The identified CHs were categorized into three broad categories based on the size of organizations as small scale organizations (SSOs), medium scale organizations (MSOs) and large scale organizations (LSOs) in the context of GSD. The classification of the CHs was used to provide a clear picture of RCM process, and their respective CHs in organization size prospective. The study also indicated the critical challenges for RCM process implementation using the standards of the factors having a frequency ≥ 50% as critical. Our findings revealed that 3 out of ten CHs are critical for the implementation of RCM process in GSD. We believe that the findings of this paper are helpful in addressing project administration related problems of RCM practitioners in GSD, which is important for the success of GSD firms.},
  keywords={Software;Organizations;Capability maturity model;Bibliographies;Standards organizations;Systematics;requirements change management (RCM);systematic literature review;“global software development (GSD)”},
